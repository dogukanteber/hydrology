{
    top
    {
        type            zeroGradient;
    }
    bottom
    {
        type            zeroGradient;
    }
    downSlope
    {
        type            zeroGradient;
    }
    upSlope
    {
        type            zeroGradient;
    }
    sides
    {
        type            empty;
    }
}


