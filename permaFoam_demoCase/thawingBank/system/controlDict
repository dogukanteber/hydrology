FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     porousUnsaturatedFoam;

startFrom       startTime;

startTime       0.;

stopAt          endTime;

endTime         10368000.;

deltaT          1.;

writeControl    adjustableRunTime;

writeInterval   1296000.;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression uncompressed;

timeFormat      fixed;

timePrecision   0;

runTimeModifiable yes;

adjustTimeStep  yes;

maxDeltaT       3600.;

libs (
       "libOpenFOAM.so"
       "libgroovyBC.so"
      );

