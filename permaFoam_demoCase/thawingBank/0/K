FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      K;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform 0.000000463;

boundaryField
{
    top
    {
    type            zeroGradient;
    }  

    downSlope
    {
    type            zeroGradient;
    }  

    bottom
    {
    type            zeroGradient;   
    }

    upSlope
    {
    type            zeroGradient;
    }   

    sides
    {
    type            empty;
    }
}


