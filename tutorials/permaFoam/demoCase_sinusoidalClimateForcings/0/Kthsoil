FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      Kthsoil;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 1 -3 -1 0 0 0];

internalField   uniform 2.9;

boundaryField
{
    top
    {
    type            zeroGradient;
    }  

    downSlope
    {
    type            zeroGradient;
    }  

    bottom
    {
    type            zeroGradient;   
    }

    upSlope
    {
    type            zeroGradient;
    }   

    sides
    {
    type            empty;
    }
}


